
<div class="row">
    <div class="col-9">
        <h2>Company list</h2>
    </div>
    <div class="col-3">
        <a asp-action="Upsert" class="btn btn-info">
            <i class="fas fa-plus"></i> Create New Company
        </a>
    </div>
    <div class="border p-3 m-3">
        <table id="tblData" class="table table-bordered table-active table-light">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Street Address</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Phone Number</th>
                    <th>Is Authorized</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>

</div>
@section scripts{
    <script src="~/js/Company.js"></script>
}
