
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Userlist</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-info" asp-page="/Account/Register"><i class="fas fa-plus">Create new user</i></a>
    </div>
</div>
<div class="table rounded p-2 m-2">
    <table id="tbldata" class="table table-bordered table-striped table-light">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section scripts
    {
    <script src="~/js/User.js"></script>
}
